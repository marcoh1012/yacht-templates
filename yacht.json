[
{
  "type": 1,
  "title": "Wyze Bridge",
  "name": "wyze-bridge",
  "description": "Create a local WebRTC, RTSP, RTMP, or HLS/Low-Latency HLS stream for most of your Wyze cameras including the outdoor, doorbell, and 2K cams.",
  "logo": "https://github.com/marcoh1012/yacht-templates/blob/666491492188e6f248d738e783b6ac7301ee85cb/icons/wyze.png?raw=true",
  "image": "mrlt8/wyze-bridge:latest",
  "note": "Streams direct from camera without additional bandwidth or subscriptions.",
  "categories": [
    "Other",
    "Tools"
  ],
  "platform": "linux",
  "restart_policy": "unless-stopped",
  "ports": [
    {
      "RTMP":"1935:1935/tcp",
      "RTSP":"8554:8554/tcp",
      "HLS":"8888:8888/tcp",
      "WebRTC":"8889:8889/tcp",
      "WebRTC/ICE":"8189:8189/udp",
      "WebUI":"5000:5000/tcp"

    }
  ],
  "network_mode": "bridge",
  "volumes": [
    {
      "container": "/config",
      "bind": "!config"
    }
  ],
  "env": [
    {
      "name": "PUID",
      "label": "PUID",
      "default": "!PUID"
    },
    {
      "name": "PGID",
      "label": "PGID",
      "default": "!PGID"
    },
    {
      "name": "WYZE_EMAIL",
      "label": "WYZE_EMAIL",
      "default": "email@email.com"
    },
    {
      "name": "WYZE_PASSWORD",
      "label": "WYZE_PASSWORD",
      "default": "password"
    },
    {
      "name": "WB_IP",
      "label": "WB_IP",
      "default": "192.168.0.143"
    },
    {
      "name": "API_ID",
      "label": "API_ID",
      "default": "changeme"
    },
    {
      "name": "API_KEY",
      "label": "API_KEY",
      "default": "changeme"
    }
  ]
},
{
  "type": 1,
  "title": "Gluetun",
  "name": "gluetun",
  "description": "Lightweight swiss-knife-like VPN client to multiple VPN service providers",
  "logo": "https://github.com/marcoh1012/yacht-templates/blob/666491492188e6f248d738e783b6ac7301ee85cb/icons/gluetun.svg?raw=true",
  "image": "qmcgaw/gluetun:latest",
  "note": "Connect To Surfshark",
  "categories": [
    "Other",
    "Tools"
  ],
  "platform": "linux",
  "restart_policy": "unless-stopped",
  "ports": [
    {
      "TDP":"6881:6881/tcp",
      "UDP":"6881:6881/udp",
      "WebUI":"8085:8085/tcp"

    }
  ],
  "network_mode": "bridge",
  "volumes": [
    {
      "container": "/gluetun",
      "bind": "!config"
    }
  ],
  "cap_add": [
    "NET_ADMIN"
  ],
  "env": [
    {
      "name": "PUID",
      "label": "PUID",
      "default": "!PUID"
    },
    {
      "name": "PGID",
      "label": "PGID",
      "default": "!PGID"
    },
    {
      "name": "VPN_SERVICE_PROVIDER",
      "label": "VPN_SERVICE_PROVIDER",
      "default": "surfshark"
    },
    {
      "name": "OPENVPN_USER",
      "label": "OPENVPN_USER",
      "default": ""
    },
    {
      "name": "OPENVPN_PASSWORD",
      "label": "OPENVPN_PASSWORD",
      "default": ""
    },
    {
      "name": "VPN_TYPE",
      "label": "VPN_TYPE",
      "default": "openvpn"
    },
    {
      "name": "SERVER_COUNTRIES",
      "label": "SERVER_COUNTRIES",
      "default": "US"
    },
    {
      "name": "SERVER_REGIONS",
      "label": "SERVER_REGIONS",
      "default": ""
    },
    {
      "name": "SERVER_CITIES",
      "label": "SERVER_CITIES",
      "default": ""
    }
    ,{
      "name": "SERVER_HOSTNAMES",
      "label": "SERVER_HOSTNAMES",
      "default": ""
    },
    {
      "name": "TZ",
      "label": "TZ",
      "default": "US/Chicago"
    }

  ]
},
{
  "type": 1,
  "title": "QBittorrent",
  "name": "qbittorrent",
  "description": "The Qbittorrent project aims to provide an open-source software alternative to ÂµTorrent. qBittorrent is based on the Qt toolkit and libtorrent-rasterbar library.",
  "logo": "https://github.com/marcoh1012/yacht-templates/blob/666491492188e6f248d738e783b6ac7301ee85cb/icons/gluetun.svg?raw=true",
  "image": "lscr.io/linuxserver/qbittorrent:latest",
  "note": "Torrent Downloads",
  "categories": [
    "Other",
    "Tools"
  ],
  "platform": "linux",
  "restart_policy": "unless-stopped",
  "network_mode": "container:gluetun",
  "volumes": [
    {
      "container": "/config",
      "bind": "!config"
    },
    {
      "container": "/downloads",
      "bind": "/downloads"
    }
  ],
  "cap_add": [
    "NET_ADMIN"
  ],
  "env": [
    {
      "name": "PUID",
      "label": "PUID",
      "default": "!PUID"
    },
    {
      "name": "PGID",
      "label": "PGID",
      "default": "!PGID"
    },
    {
      "name": "TZ",
      "label": "TZ",
      "default": "US/Chicago"
    },
    {
      "name": "WEBUI_PORT",
      "label": "WEBUI_PORT",
      "default": "8085"
    }
  ],
  "depends_on": [
    "gluetun"
  ]
},
{
  "type": 1,
  "title": "Jellyfin",
  "name": "jellyfin",
  "description": "Jellyfin is a Free Software Media System that puts you in control of managing and streaming your media. It is an alternative to the proprietary Emby and Plex, to provide media from a dedicated server to end-user devices via multiple apps.",
  "logo": "https://raw.githubusercontent.com/SelfhostedPro/selfhosted_templates/master/Images/jellyfin.png",
  "image": "lscr.io/linuxserver/jellyfin:latest",
  "categories": [
    "Video",
    "Music",
    "Photos",
    "Custom"
  ],
  "add_group":106,
  "platform": "linux",
  "restart_policy": "unless-stopped",
  "ports": [
    {
      "WebUI": "8096:8096/tcp",
      "HTTPS WebUI": "8920:8920/tcp",
      "DNLA": "1900:1900/udp",
      "Discovery": "7359:7359/udp"
    }
  ],
  "volumes": [
    {
      "container": "/config",
      "bind": "!config/Jellyfin"
    },
    {
      "container": "/data/tvshows",
      "bind": "!tv"
    },
    {
      "container": "/data/movies",
      "bind": "!movies"
    },
    {
      "container": "/data/custom",
      "bind": "!custom"
    },
    {
      "container": "/data/intros",
      "bind": "!intros"
    }
  ],
  "env": [
    {
      "name": "PUID",
      "label": "PUID",
      "default": "!PUID"
    },
    {
      "name": "PGID",
      "label": "PGID",
      "default": "!PGID"
    },
    {
      "name": "TZ",
      "label": "TZ",
      "default":"America/Chicago"
    }
  ]
},
{
  "type": 1,
  "title": "Snippet-Box",
  "name": "snippet-box",
  "description": "Snippet Box is a simple self-hosted app for organizing your code snippets. It allows you to easily create, edit, browse and manage your snippets in various languages. With built-in Markdown support, Snippet Box makes it very easy to add notes or simple documentation to your code.",
  "logo": "https://raw.githubusercontent.com/SelfhostedPro/selfhosted_templates/master/Images/jellyfin.png",
  "image": "pawelmalak/snippet-box:latest",
  "categories": [
    "Tools"
  ],
  "platform": "linux",
  "restart_policy": "unless-stopped",
  "ports": [
    {
      "WebUI": "5050:5000/tcp"
    }
  ],
  "volumes": [
    {
      "container": "/app/data",
      "bind": "!config/snippet-box"
    }
  ],
  "env": [
    {
      "name": "PUID",
      "label": "PUID",
      "default": "!PUID"
    },
    {
      "name": "PGID",
      "label": "PGID",
      "default": "!PGID"
    },
    {
      "name": "TZ",
      "label": "TZ",
      "default":"America/Chicago"
    }
  ]
}
]