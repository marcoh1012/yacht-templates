    },
    {
      "name": "OPENVPN_USER",
      "label": "OPENVPN_USER",
      "default": ""
    },
    {
      "name": "OPENVPN_PASSWORD",
      "label": "OPENVPN_PASSWORD",
      "default": ""
    },
    {
      "name": "VPN_TYPE",
      "label": "VPN_TYPE",
      "default": "openvpn"
    },
    {
      "name": "SERVER_COUNTRIES",
      "label": "SERVER_COUNTRIES",
      "default": "US"
    },
    {
      "name": "SERVER_REGIONS",
      "label": "SERVER_REGIONS",
      "default": ""
    },
    {
      "name": "SERVER_CITIES",
      "label": "SERVER_CITIES",
      "default": ""
    }
    ,{
      "name": "SERVER_HOSTNAMES",
      "label": "SERVER_HOSTNAMES",
      "default": ""
    },
    {
      "name": "TZ",
      "label": "TZ",
      "default": "US/Chicago"
    }

  ]
