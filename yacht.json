    },
    {
      "container": "/data/movies",
      "bind": "!movies"
    },
    {
      "container": "/data/custom",
      "bind": "!custom"
    },
    {
      "container": "/data/intros",
      "bind": "!intros"
    }
  ],
  "env": [
    {
      "name": "PUID",
      "label": "PUID",
      "default": "!PUID"
    },
    {
      "name": "PGID",
      "label": "PGID",
      "default": "!PGID"
    },
    {
      "name": "TZ",
      "label": "TZ",
      "default":"America/Chicago"
    }
  ]
},
